package projetofinal;


abstract class SuperClasseAnimal {
  
    protected int id;
    protected String nome;
    protected int idade;
    protected boolean sexo;
    protected String historicovacinal;
    
    public void setId(int nId){  
        this.id = nId;  
}
    public void setNome(String nNome){  
        this.nome = nNome;  
    }  
    
     public void setIdade(int nIdade){  
        this.idade = nIdade;  
    }  
     
     public void setSexo(boolean nSexo){  
        this.sexo = nSexo;  
    }
     public void setHistoricovacinal(String nHistoricovacinal){
             this.historicovacinal = nHistoricovacinal;
     }
     //exemplo de m√©todo get
     /* public int getId(){  
        return nroIdentificacao;  
    } */
     
     public int getId(){
     return id;
     }
     
     public String getNome(){
     return nome;
     }
     
     public int getIdade(){
     return idade;
     }
     
     public boolean getSexo(){
     return sexo;
     }
     
     public String getHistoricovacinal(){
     return historicovacinal;
     }
    
     

    
}
